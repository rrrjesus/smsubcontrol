{
  "name": "smsub/smsubcontrol",
  "description": "Sistema de Controle SMSUB",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Rodolfo Romaioli Ribeiro de Jesus",
      "email": "rodolfo.romaioli@gmail.com",
      "homepage": "http://localhost/smsubcontrol/",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "source/"
    },
    "files": [
      "source/Boot/Config.php",
      "source/Boot/Helpers.php",
      "source/Boot/Minify/Web.php",
      "source/Boot/Minify/Beta.php"
    ]
  },
  "require": {
    "php": "8.2.*",
    "ext-pdo": "8.2.*",
    "phpmailer/phpmailer": "6.*",
    "league/plates": "3.*",
    "matthiasmullie/minify": "^1.3",
    "coffeecode/paginator": "2.*",
    "coffeecode/router": "2.*",
    "coffeecode/optimizer": "2.*",
    "coffeecode/cropper": "2.*",
    "coffeecode/uploader": "2.*",
    "twbs/bootstrap": "5.3.3"
  }
}
